{% extends "layout.njk" %}

{% block content %}


<section>
  <div class="card__course" id="{{item.id}}">
    <div class="card__image">
      <img src="{{item.url}}" alt="{{item.name}}">
    </div>
    <div class="card__button">
      <button><a href="https://rocketseat.com.br/{{item.id}}" target="_blank">Saiba mais</a></button>
    </div>
  </div>
</section>

<script src="scripts.js"></script>

{% endblock %}